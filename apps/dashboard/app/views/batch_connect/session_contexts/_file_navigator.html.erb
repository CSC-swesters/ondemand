<%
content_for :head do
  javascript_include_tag 'navigator/index', nonce: true
end
%>

<%= label attrib.id, "#{field_options[:label]}" %>
<%= text_field :batch_connect_session_context, attrib.id, class: 'form-control' %>
<%= button_tag 'Select Path', type: 'button', class: 'btn btn-primary', style: 'margin-top: 15px;', id: 'select-nav-path' %>
<div id="container-directory-contents" style="display:none;">
    <table class="table table-striped table-condensed w-100" id="directory-contents">
        <thead>
        <tr>
            <th>Type</th>
            <th>Name</th>
        </tr>
        </thead>
        <tbody>
        </tbody>
    </table>
</div>
